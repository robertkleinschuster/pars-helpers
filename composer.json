{
  "name": "pars/pars-helpers",
  "description": "",
  "license": "BSD-3-Clause",
  "keywords": [
    "pars"
  ],
  "support": {
    "docs": "https://docs.parsphp.org/pars-helpers/",
    "issues": "https://github.com/pars/pars-helpers/issues",
    "source": "https://github.com/pars/pars-helpers",
    "rss": "https://github.com/pars/pars-helpers/releases.atom",
    "forum": "https://discourse.parsphp.org/"
  },
  "require": {
    "php": "^7.4",
    "pars/pars-core": "dev-master"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Pars\\Helper\\": "src//"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParsTest\\Helper\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "classmap-authoritative": true
  },
  "scripts": {
  }
}
